{
	"include": ["src", "vite.config.ts"],
	"compilerOptions": {
		"verbatimModuleSyntax": true,
		"strict": true,
		"noEmit": true,
		"moduleResolution": "Bundler",
		"module": "ESNext",
		"target": "ESNext",
		"lib": ["ESNext", "DOM", "DOM.Iterable"],
		"types": ["vite/client", "react/experimental"],
		"jsx": "react-jsx"
	}
}
